local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Scary heater", "Ocean")
local Tab = Window:NewTab("game")
local Section = Tab:NewSection("get eps")
Section:NewButton("eps", "ButtonInfo", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/A84cMe8w", true))()
end)
local Section = Tab:NewSection("autofarm")
Section:NewButton("win every round as survivor", "warning you can't turn off the autofarm", function()
    getgenv().ee = false
while wait() do
if getgenv().ee == true then
 
local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
local location = CFrame.new( 113.718, 153.347, 4798)
local humanoid = game.Players.LocalPlayer.Character.Humanoid
humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
wait(0.1)
pl.CFrame = location
end
end


end)
Section:NewButton(" get anti afk", "use this if you want autofarm", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/m15DXJEf", true))()
end)
local Tab = Window:NewTab("teleporter")
local Section = Tab:NewSection("lobby area")
Section:NewButton("lobby", "ButtonInfo", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/jybqJuB2"))()
end)
Section:NewButton("house", "ButtonInfo", function()
     loadstring(game:HttpGet("https://pastebin.com/raw/jZctuSPP"))()
end)
--hey lol why can see this ?!
local Tab = Window:NewTab("character")
local Section = Tab:NewSection("speed")
Section:NewSlider("walk", "SliderInfo", 500, 1, function(v) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)
