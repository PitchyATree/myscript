     getgenv().FartingAt3AM = bool
     while wait(0.1) do
         if getgenv().FartingAt3AM == true then
             local me = game.Players.LocalPlayer.Character.HumanoidRootPart
for i,v in pairs(game.Workspace.Map.Presents.Present:GetDescendants()) do
if v.Name == "Box" then
v.CFrame = me.CFrame
end
end
end
end 
end)
