SelectedEgg = ""
Eggs = {}
for i,v in pairs(game:GetService("Workspace").Eggs["Egg Capsules"]:GetChildren()) do
    table.insert(Eggs,v.Name)
end


local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Shoes Simulator PitchHub", "Ocean")
local Tab = Window:NewTab("Autofarm")
local Section = Tab:NewSection("")
Section:NewButton("on", "ButtonInfo", function()
    getgenv().autochaussure11 = true

        while wait() do
        if getgenv().autochaussure11 == true then
            local A_1 = 4
            game:GetService("Players").LocalPlayer.Character.Shoes.PutShoe:FireServer()
            end
            end
end)
Section:NewButton("off", "ButtonInfo", function()
    getgenv().autochaussure11 = false
end)
local Tab = Window:NewTab("buy")
local Section = Tab:NewSection("eggs (by NAYPRAMXV)")
Section:NewDropdown("Selected Egg", "DropdownInf", Eggs, function(currentOption)
  SelectedEgg = currentOption
end)
Section:NewToggle("Auto Open Egg", "ToggleInfo", function(state)
_G.EGG = state
 
while _G.EGG do wait()
-- Script generated by SimpleSpy - credits to exx#9394 --and NAYPRAMXV
 
local args = {
    [1] = SelectedEgg
}
 
game:GetService("ReplicatedStorage").Remote._PurchaseEgg:InvokeServer(unpack(args))
end
end)
local Section = Tab:NewSection("crates")
Section:NewButton("crate1-10k", "ButtonInfo", function()
local A_1 = "OpenBox"
local A_2 = game:GetService("Workspace").Boxes.Box1
local Event = game:GetService("ReplicatedStorage").Remote["_PlayerRequest"]
Event:InvokeServer(A_1, A_2)

end)
Section:NewButton("crate2_25k", "ButtonInfo", function()
local A_1 = "OpenBox"
local A_2 = game:GetService("Workspace").Boxes.Box2
local Event = game:GetService("ReplicatedStorage").Remote["_PlayerRequest"]
