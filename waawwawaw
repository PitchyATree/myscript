local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/PitchyATree/roblox-exploit-library/main/turtle%20library%20red%20version"))()
local window = library:Window("PitchHub - bear* quidz tp")

window:Toggle("quidz tp", true, function(bool)  -- you can change window to the ui you want the toggle in 
    getgenv().quidz =  bool
    
    while wait(1) do
		if getgenv().quidz == true then

local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
			local location = CFrame.new(407.5, 27, -27.5)
			local humanoid = game.Players.LocalPlayer.Character.Humanoid
			humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
			wait(0.1)
			pl.CFrame = location
    
    end
    end-- bool is true or false depending on the state of the toggle
end)
