cb:Button("temple island island", function()     
local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
local location = CFrame.new(1204.4, 54.6077, 1008.15)
local humanoid = game.Players.LocalPlayer.Character.Humanoid
humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
wait(0.1)
pl.CFrame = location
end)

cba:Button("get noclip(c)", function()
noclip = false
game:GetService('RunService').Stepped:connect(function()
if noclip then
game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end
end)
plr = game.Players.LocalPlayer
mouse = plr:GetMouse()
mouse.KeyDown:connect(function(key)
 
if key == "c" then
noclip = not noclip
game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end
end)
end)

bc:Button("plot shop (old)", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/PitchyATree/myscript/main/timber%20plots%20shop%20by%20pitchhub", true))()
end)

ac:Button("PitchHub(clic2link)", function()
setclipboard("https://www.youtube.com/channel/UCyPwttNCvLm3f9Ilz_M569g/featured")
end)






library:Keybind("P")
