local library = loadstring(game:HttpGet('https://raw.githubusercontent.com/PitchyATree/roblox-exploit-library/main/turtle%20library'))()
local ba = library:Window("auras")
local cb = library:Window("teleporters")
local cba = library:Window("player")
local bc = library:Window("more")
local ac = library:Window("credit")

ba:Toggle("Mini Tree", true, function(bool)  -- you can change window to the ui you want the toggle in 
    getgenv().mini = bool

while wait() do
        if getgenv().mini == true then
            local args = {
    [1] = 3
}

game:GetService("ReplicatedStorage").Communication.Remotes.HitMiniMegaTree:FireServer(unpack(args))
            end
            end
end)








ba:Toggle("Middle Tree", true, function(bool)  -- you can change window to the ui you want the toggle in 
    getgenv().middle = bool

while wait() do
        if getgenv().middle == true then
            local A_1 = 1
            local Event = game:GetService("ReplicatedStorage").Communication.Remotes.HitMegaTree
            Event:FireServer(A_1)
            end
            end
end)

ba:Toggle("Volcano Tree", true, function(bool)  -- you can change window to the ui you want the toggle in 
    getgenv().volcano = bool

while wait() do
        if getgenv().volcano == true then
            local A_1 = 2
            local Event = game:GetService("ReplicatedStorage").Communication.Remotes.HitMegaTree
            Event:FireServer(A_1)
            end
            end
end)

cb:Button("middle island", function()     
local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
local location = CFrame.new(-47.6234, 15.5373, 134.501)
local humanoid = game.Players.LocalPlayer.Character.Humanoid
humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
wait(0.1)
pl.CFrame = location
end)

